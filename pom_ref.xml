			<id>linux</id>
			<activation> 
				<os> 
					<family>unix</family> 
				</os> 
			</activation> 
			<properties>
				<surefire.forkCount>4</surefire.forkCount>
				<surefire.reuseForks>true</surefire.reuseForks>
			</properties>
		</profile>
		<profile>
			<id>windows</id>
			<activation> 
				<os> 
					<family>Windows</family> 
				</os> 
			</activation> 
			<properties>
				<!-- Have to set forkCount=0 to avoid class loader problems due to long classpath on Windows -->
				<surefire.forkCount>0</surefire.forkCount>
				<surefire.reuseForks>false</surefire.reuseForks>
			</properties>
		</profile>
	</profiles>
</project>